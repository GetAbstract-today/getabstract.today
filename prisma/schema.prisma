// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Url {
  id        String   @id @default(cuid())
  url       String
  date      DateTime
  category  String
  createdAt DateTime @default(now())
}

model Newsletter {
  id        String   @id @default(cuid())
  content   String   // Full newsletter as Markdown
  category  String
  createdAt DateTime @default(now())
}
